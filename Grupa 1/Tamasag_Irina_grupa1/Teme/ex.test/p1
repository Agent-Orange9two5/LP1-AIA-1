#include <iostream>
#include<fstream>
using namespace std;
ofstream fout("tema.test.p1");
int main()
{
    int v[100],n,i, s=0;
    cout<<"Marimea vect este: ";
    cin>>n;
    cout<<endl<<"Introduceti numerele din vector:";
    for(i=0;i<n;i++)
    {
        cin>>v[i];
        if(v[i]%2==0)
            s+=v[i];
    }
    fout<<"Suma numerelor pare este:"<<" "<<s;
    return 0;
}
