#include <iostream>

using namespace std;

int a[100][100], n;
int main()
{
  cout <<"Introduceti numarul de linii/coloane: ";
  cin >> n;
  for (int i=0; i<n; i++)
  {
   cout <<"Linia "<< i+1<< ": ";
  for (int j=0; j<n; j++)
  {
  cin >> a[i][j];
  }
cout<<endl;
  //Am citit matricea

  for (int i=0; i<n; i++)
  {
  for (int j=0; j<n; j++)
  {
  cout << a[i][j]<<" ";
  }
  cout<<endl;
  //Am afisat matricea
  }
  return 0;
}
}
/*Salutare, am incercat sa afisez o matrice patratica de 4 dar nu inteleg de ce imi citeste doar prima linie ?*/
